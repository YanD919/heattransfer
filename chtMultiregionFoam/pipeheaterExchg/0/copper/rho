FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/copper";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 8000;

boundaryField
{
    copperend1
    {
        type            calculated;
        value           uniform 8000;
    }
    copperend2
    {
        type            calculated;
        value           uniform 8000;
    }
    copper_to_shell
    {
        type            calculated;
        value           uniform 0;
    }
    copper_to_tube
    {
        type            calculated;
        value           uniform 0;
    }
}


