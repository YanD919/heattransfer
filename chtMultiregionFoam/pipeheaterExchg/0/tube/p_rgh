FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/tube";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 100000;

boundaryField
{
    tubeend1
    {
        type            fixedValue;
        value           uniform 100000;
    }
    tubeend2
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    tube_to_copper
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }
}


