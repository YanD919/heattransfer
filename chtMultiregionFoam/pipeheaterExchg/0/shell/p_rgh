FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/shell";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 100000;

boundaryField
{
    outershell
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }
    shellend1
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }
    shellend2
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }
    inletshell
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }
    outletshell
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }
    inlet
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 100000;
    }
    shell_to_copper
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }
}


