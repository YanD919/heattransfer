FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/shell";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    cyclic
    {
        type            calculated;
        value           $internalField;
    }
    cyclicAMI
    {
        type            calculated;
        value           $internalField;
    }
    cyclicACMI
    {
        type            calculated;
        value           $internalField;
    }
    cyclicSlip
    {
        type            calculated;
        value           $internalField;
    }
    empty
    {
        type            calculated;
        value           $internalField;
    }
    nonuniformTransformCyclic
    {
        type            calculated;
        value           $internalField;
    }
    processor
    {
        type            calculated;
        value           $internalField;
    }
    processorCyclic
    {
        type            calculated;
        value           $internalField;
    }
    symmetryPlane
    {
        type            calculated;
        value           $internalField;
    }
    symmetry
    {
        type            calculated;
        value           $internalField;
    }
    wedge
    {
        type            calculated;
        value           $internalField;
    }
    overset
    {
        type            calculated;
        value           $internalField;
    }
    ".*"
    {
        type            calculated;
        value           uniform 0;
    }
}


