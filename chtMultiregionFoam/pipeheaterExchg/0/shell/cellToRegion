FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/shell";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    outershell
    {
        type            zeroGradient;
    }
    shellend1
    {
        type            zeroGradient;
    }
    shellend2
    {
        type            zeroGradient;
    }
    inletshell
    {
        type            zeroGradient;
    }
    outletshell
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    shell_to_copper
    {
        type            calculated;
        value           uniform 0;
    }
}


