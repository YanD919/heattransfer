// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

boundary
{
}

T
{
    internalField   uniform 293.15;

    boundaryField
    {
        ".*"
        {
            type            zeroGradient;
            value           $internalField;
        }
        "procBoundary.*"
        {
            type            processor;
        }
        "copper_to_.*"
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;
            Tnbr            T;
            kappaMethod     solidThermo;
            value           $internalField;
        }
    }
}


