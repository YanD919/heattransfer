FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "2/tube";
    object      Qdot;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    tubeend1
    {
        type            calculated;
        value           uniform 0;
    }
    tubeend2
    {
        type            calculated;
        value           uniform 0;
    }
    tube_to_copper
    {
        type            calculated;
        value           uniform 0;
    }
}


